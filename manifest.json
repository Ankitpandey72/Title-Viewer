{
  "manifest_version": 3,
  "name": "Title Viewer",
  "description": "It Picks the title of current tab.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/title.png"
  },
  "icons": {
    "16": "images/title.png",
    "32": "images/title.png",
    "52": "images/title2.png"
  },
  
  "permissions": [
    "activeTab"
  ]
}